r\n    setPosition() {\r\n      this.imageStore.forEach((o) => {\r\n        o.mesh.position.y =\r\n          this.currentScroll - o.top + this.height / 2 - o.height / 2;\r\n        o.mesh.position.x = o.left - this.width / 2 + o.width / 2;\r\n      });\r\n    }\r\n    setupRezize() {\r\n      window.addEventListener(\"resize\", this.rezize.bind(this));\r\n    }\r\n  \r\n    addObject() {\r\n      this.geometry = new THREE.PlaneBufferGeometry(100, 100, 10, 10);\r\n      // this.material = new THREE.MeshNormalMaterial();\r\n  \r\n      this.material = new THREE.ShaderMaterial({\r\n        uniforms: {\r\n          time: { value: 0 },\r\n          waterTexture: { value: new THREE.TextureLoader().load(water) },\r\n        },\r\n        side: THREE.DoubleSide,\r\n        fragmentShader: fragment,\r\n        vertexShader: vertex,\r\n        wireframe: true,\r\n      });\r\n  \r\n      this.mesh1 = new THREE.Mesh(this.geometry, this.material);\r\n      this.mesh2 = new THREE.Mesh(this.geometry, this.material);\r\n      this.scene.add(this.mesh1);\r\n      // this.scene.add(this.mesh2);\r\n      // this.mesh2.position.set(0,0,0)\r\n    }\r\n  \r\n    rezize() {\r\n      this.width = this.container.offsetWidth;\r\n      this.height = this.container.offsetHeight;\r\n      this.renderer.setSize(this.width, this.height);\r\n      this.camera.aspect = this.width / this.height;\r\n      this.camera.updateProjectionMatrix();\r\n    }\r\n  \r\n    render() {\r\n      this.time += 0.05;\r\n  \r\n      // this.mesh.rotation.x = this.time / 2000;\r\n      // this.mesh.rotation.y = this.time / 1000;\r\n      // this.material.uniforms.time.value = this.time;\r\n  \r\n  this.materials.forEach(m=>{\r\n    m.uniforms.time.value = this.time;\r\n  })\r\n  \r\n      this.scroll.render();\r\n      this.currentScroll = this.scroll.scrollToRender;\r\n      this.setPosition();\r\n      this.renderer.render(this.scene, this.camera);\r\n      window.requestAnimationFrame(this.render.bind(this));\r\n